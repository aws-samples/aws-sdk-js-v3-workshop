{
  "name": "aws-sdk-js-v3-workshop-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for the AWS JS SDK v3 workshop",
  "dependencies": {
    "@aws-crypto/sha256-browser": "^0.1.0-preview.1",
    "@aws-sdk/client-cognito-identity-browser": "^0.1.0-preview.2",
    "@aws-sdk/client-s3-browser": "^0.1.0-preview.2",
    "@aws-sdk/credential-provider-cognito-identity": "^0.1.0-preview.2",
    "@aws-sdk/s3-request-presigner": "^0.1.0-preview.2",
    "@aws-sdk/util-create-request": "^0.1.0-preview.2",
    "@aws-sdk/util-format-url": "^0.1.0-preview.1",
    "@reach/router": "^1.2.1",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.2"
  },
  "scripts": {
    "deploy": "aws cloudformation deploy --template-file template.yaml --stack-name ${AWS_JS_SDK_ID}-frontend --capabilities CAPABILITY_IAM",
    "identity-pools": "aws cognito-identity list-identity-pools --max-results 20",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "clean": "aws cloudformation delete-stack --stack-name ${AWS_JS_SDK_ID}-frontend"
  },
  "keywords": [
    "aws",
    "JavaScript",
    "TypeScript",
    "SDK",
    "v3",
    "workshop",
    "Node.js"
  ],
  "author": "Kamat, Trivikram",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@types/reach__router": "^1.2.5",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "react-scripts": "3.2.0",
    "typescript": "~3.6.4"
  }
}
